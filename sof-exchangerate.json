{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentStage": {
      "type": "string"
    },
    "resourceName": {
      "type": "string"
    }
  },
  "variables": {
    "templateBaseUrl": "https://raw.githubusercontent.com/acckrs/crayon1/master/",
    "aiTemplateUrl": "[uri(variables('templateBaseUrl'), 'template-applicationinsights.json')]"
  },
  "resources": [
    {
      "name": "linkedDeployment1",
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2020-10-01",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[variables('aiTemplateUrl')]",
          "contentVersion": "1.0.0.0"
        },
      "parametersLink": {
        "uri": "[uri(variables('templateBaseUrl'), 'sof-exchangerate-dit-parameters.json')]",
        "contentVersion": "1.0.0.0"
      }
      }
    }
  ]
}